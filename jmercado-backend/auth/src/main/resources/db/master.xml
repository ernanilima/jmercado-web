<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.4.xsd">

    <property name="uuid" value="uuid" dbms="postgresql, h2"/>
    <property name="password" value="java.sql.Types.VARCHAR(255)" dbms="postgresql, h2"/>
    <property name="varchar_75" value="java.sql.Types.VARCHAR(75)" dbms="postgresql, h2"/>
    <property name="varchar_50" value="java.sql.Types.VARCHAR(50)" dbms="postgresql, h2"/>
    <property name="varchar_20" value="java.sql.Types.VARCHAR(20)" dbms="postgresql, h2"/>
    <property name="datetime" value="java.sql.Types.TIMESTAMP" dbms="postgresql, h2"/>
    <property name="zipcode" value="java.sql.Types.VARCHAR(8)" dbms="postgresql, h2"/>
    <property name="int" value="java.sql.Types.NUMERIC(20,0)" dbms="postgresql, h2"/>
    <property name="logical" value="java.sql.Types.BOOLEAN" dbms="postgresql, h2"/>

    <includeAll path="db/changelog" relativeToChangelogFile="false"/>

</databaseChangeLog>