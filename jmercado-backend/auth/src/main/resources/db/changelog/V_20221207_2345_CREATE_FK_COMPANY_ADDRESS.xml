                                 initiallyDeferred="false" onDelete="RESTRICT" onUpdate="RESTRICT"
                                 referencedTableName="ADDRESS" referencedColumnNames="ID_ADDRESS" validate="true"/>
    </changeSet>
</databaseChangeLog>